{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "ESNext"
    ],
    "allowJs": true,
    "checkJs": true,
    "strict": false,
    "noImplicitAny": false,
    "noEmit": true,
    "baseUrl": ".",
    "typeRoots": [
      "./types"
    ],
    "skipLibCheck": true,
	"allowImportingTsExtensions": true
  }
}
